#include<conio.h>
#include<stdio.h>
#include<stdlib.h>
 int main()
{
	int money;
	printf("Introduce cantidad de dinero: ");
	scanf("%d",&money);
	if(money>=1000)
	{	
		printf("\n %d billete de 1000 pesos",money/1000);
		money=money%1000;
	}
	if(money>=500)
	{	
		printf("\n %d billete de 500 pesos",money/500);
		money=money%500;
	}
	if(money>=200)
	{	
		printf("\n %d billete de 200 pesos",money/200);
		money=money%200;
	}
	if(money>=100)
	{	
		printf("\n %d billete de 100 pesos",money/100);
		money=money%100;
	}
	if(money>=50)
	{	
		printf("\n %d billete de 50 pesos",money/50);
		money=money%50;
	}
		if(money>=20)
	{	
		printf("\n %d billete de 20 pesos",money/20);
		money=money%20;
	}
		if(money>=10)
	{	
		printf("\n %d moneda de 10 pesos mexicanos",money/10);
		money=money%10;
	}
		if(money>=5)
	{	
		printf("\n %d moneda de 5 pesos mexicanos",money/5);
		money=money%5;
	}
		if(money>=2)
	{	
		printf("\n %d moneda de 2 pesos mexicanos",money/2);
		money=money%2;
	}
		if(money>=1)
	{	
		printf("\n %d monedade 1 pesos mexicanos",money/1);
		money=money%1;
	}
	printf ("fin del programa");
	return 0;
	getch();
}
